{
  "name": "egg-development-proxyworker",
  "version": "1.1.1",
  "description": "A proxy worker for debugging worker on egg.",
  "eggPlugin": {
    "name": "proxyworker",
    "env": [
      "local"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-proxyworker"
  ],
  "dependencies": {
    "debug": "^2.6.8",
    "egg-logger": "^1.6.0",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "egg": "^1.4.0",
    "egg-bin": "^3.4.2",
    "egg-ci": "^1.7.0",
    "egg-mock": "^3.7.2",
    "eslint": "^3.19.0",
    "eslint-config-egg": "^4.2.0",
    "ko-sleep": "^1.0.3",
    "proxy": "^0.2.4",
    "semver": "^5.3.0",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
