{
  "name": "egg-development-proxyworker",
  "version": "0.2.1",
  "description": "A proxy worker for debugging worker on egg.",
  "eggPlugin": {
    "name": "proxyworker",
    "env": [
      "local"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-proxyworker"
  ],
  "devDependencies": {
    "autod": "^2.7.1",
    "egg": "^0.7.0",
    "egg-bin": "^1.10.0",
    "egg-ci": "^1.1.0",
    "egg-mock": "^2.0.0",
    "eslint": "^3.13.1",
    "eslint-config-egg": "^3.2.0",
    "proxy": "^0.2.4",
    "supertest": "^2.0.1",
    "webstorm-disable-index": "^1.1.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "clean": "rimraf coverage",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run clean && npm run lint && npm run cov",
    "autod": "autod"
  },
  "files": [
    "index.js",
    "app.js",
    "agent.js",
    "config",
    "app",
    "lib"
  ],
  "ci": {
    "version": "6, 7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okoala/egg-development-proxyworker.git"
  },
  "homepage": "https://github.com/okoala/egg-development-proxyworker#readme",
  "author": "okoala <dapixp@gmail.com>",
  "license": "MIT"
}
